# Local Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=adapt_agent_gpt_db
DB_USER=postgres
DB_PASSWORD=actual_local_db_password

# GCP Configuration
GCP_PROJECT_ID=adapt-434803
GCP_REGION=us-central1
CLOUD_SQL_INSTANCE_NAME=adapt-agent-gpt-db
CLOUD_SQL_DATABASE_NAME=adapt_agent_gpt_db
CLOUD_SQL_USER=postgres
CLOUD_SQL_PASSWORD=your_gcp_db_password

# Cloud SQL connection name
CLOUD_SQL_CONNECTION_NAME=${GCP_PROJECT_ID}:${GCP_REGION}:${CLOUD_SQL_INSTANCE_NAME}

# Redis Configuration
REDIS_HOST=10.0.0.3
REDIS_PORT=6379

# Firestore Configuration
FIRESTORE_COLLECTION=adapt_agent_data

# Vertex AI Configuration
VERTEX_AI_ENDPOINT=us-central1-aiplatform.googleapis.com

# Other configurations
NODE_ENV=production
PORT=8080
JWT_SECRET=your_jwt_secret
CORS_ORIGIN=https://your-frontend-domain.com

# Google Cloud Storage
GCS_BUCKET_NAME=adapt-agent-gpt-storage

# Language Model Configuration
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
PALM_API_KEY=your_palm_api_key
LLAMA_API_ENDPOINT=your_llama_api_endpoint
FALCON_API_ENDPOINT=your_falcon_api_endpoint

# Model-specific configurations
GPT_3_5_TURBO_MODEL=gpt-3.5-turbo
GPT_4_MODEL=gpt-4
CLAUDE_MODEL=claude-v1
LLAMA_MODEL=llama-2-70b
FALCON_MODEL=falcon-40b
PALM_MODEL=palm-2

# Add any other necessary environment variables here