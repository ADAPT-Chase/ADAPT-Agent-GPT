# Server Configuration
PORT=5001
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your_jwt_secret_here

# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

# Cloud SQL Configuration
CLOUD_SQL_INSTANCE_NAME=your-instance-name
CLOUD_SQL_DATABASE_NAME=adapt_agent_gpt_db
CLOUD_SQL_USER=postgres
CLOUD_SQL_PASSWORD=your_cloud_sql_password

# Cloud Memorystore (Redis) Configuration
REDIS_HOST=your-redis-host
REDIS_PORT=6379

# Firestore Configuration
FIRESTORE_COLLECTION=adapt_agent_data

# Vertex AI Configuration
VERTEX_AI_ENDPOINT=your-vertex-ai-endpoint

# Email Configuration
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=your-email@example.com
EMAIL_PASS=your-email-password

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000

# The DATABASE_URL and REDIS_URL will be constructed in gcp_config.js
# DATABASE_URL=
# REDIS_URL=